            <lb xml:id="uuid_bc2fc7f3-c860-411d-b40f-95cb09134427"/>219 <rs ref="#pmb146" type="place">FRANKFURTMAIN</rs> F 8259 28 22/1 20.35<lb xml:id="uuid_10eda4c4-6284-44e9-8cf8-f10cfe5542a2"/>= <rs ref="#pmb37240" type="institution">KOMOEDIENHAUS</rs>
