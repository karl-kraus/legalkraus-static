         <p xml:id="uuid_9ebf6c95-da96-49fb-9364-aaad1661aca4">
            <lb xml:id="uuid_78cddf8d-15b4-4d99-a323-ea4f833e98ea"/><note type="paratext" resp="law-firm"><rs type="person" ref="#pmb11988">Kraus</rs>
