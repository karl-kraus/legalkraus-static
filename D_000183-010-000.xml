         <pb xml:id="D_000183-010-000_p001" facs="#D_000183-010-000-facs001-l001-p001" n="1"/>
         <p xml:id="uuid_62fbc63c-a5d5-426f-abe0-0e8a10528608">
            <lb xml:id="uuid_e6aec0c7-b7b3-4b2c-8574-70a92d0f3df2"/><rs type="place" ref="#pmb50">Wien</rs>, 6.4.35 </p>
         <p xml:id="uuid_69c3bc29-f3d1-41be-b8c2-9d6b66e7a556">
            <lb xml:id="uuid_0f53a0f3-8196-472a-aa27-1f173322807b"/>Im Auftrag<lb xml:id="uuid_47a00498-d7ad-4ce9-bd73-5c7a3165d3fe"/><rs type="institution" ref="#pmb37055">Security First National Bank</rs>
            <lb xml:id="uuid_c3241ae4-eea4-4633-add1-8dead43ac63b"/><rs type="place" ref="#pmb37116">Los Angeles, Cal.</rs>
         </p>
         <p xml:id="uuid_aefc72b1-4995-4d20-ac8f-89457155b206">
            <lb xml:id="uuid_2d7f97a5-9b33-4d78-8ce2-b1e30cdf4e8f"/>Radio Nr. 9<lb xml:id="uuid_53b70485-3eb5-4009-810e-6cce2ddca8b7"/>Apr. 4th </p>
         <p xml:id="uuid_dd7b9021-46d1-4d4a-bb6b-5de23cea4554">
            <lb xml:id="uuid_f9131c94-79b8-4db2-a98c-a07aa983a25d"/>hielten wir Ihnen<lb xml:id="uuid_94ab88d7-af1d-495a-9f71-da1b54cdb7df"/>wegen <rs type="person" ref="#pmb39006">Lorre</rs>
            <lb xml:id="uuid_d5ee2d62-f31c-4d27-960b-a22619536107"/>den Betrag von Doll. 50.– in
                unserem Parteienbüro, <rs type="place" subtype="address" ref="#pmb55450">Wien I.,
                    Schottengasse 6</rs>, zur Verfügung, welchen Sie<lb xml:id="uuid_4e17917b-363a-4617-93ff-4254cb531018"/>heute behoben haben. </p>
         <p xml:id="uuid_97735f0d-6898-44ad-b8dd-761351d45077">
            <lb xml:id="uuid_da03ca14-eed7-4d76-915c-d29a7a89057d"/>Herrn<lb xml:id="uuid_9665f8e6-244e-4c67-bb0a-fb39b4f3872a"/>Dr. <rs type="person" ref="#pmb38909">Oskar Samek</rs>
            <lb xml:id="uuid_97e828bb-3bf0-4770-aad1-b64dad88648d"/><rs type="place" subtype="address" ref="#pmb52780">Wien, XIV.<lb xml:id="uuid_07ea016e-4423-4e2f-87d1-8d3575c8dd76"/>Reindorfgasse 18</rs>
         </p>
         <p xml:id="uuid_4567dbb3-f538-4cb8-8de3-e093e912acfd">
            <lb xml:id="uuid_fcccea1a-75e1-4950-bb9a-7b63c372f05a"/>[<rs type="person">Unterschrift</rs>] </p>
         <pb xml:id="D_000183-010-000_p002" facs="#D_000183-010-000-facs001-l001-p002" n="2"/>
         <p xml:id="uuid_050fdce5-ca82-4eb1-aaf0-0bc63092b8c7">
            <lb xml:id="uuid_14536d7f-6844-439d-9bb8-e9a283a825be"/><note type="paratext" resp="law-firm"><rs type="person" ref="#pmb11988">Kraus</rs> – <rs type="person" ref="#pmb39006">Lorre</rs><lb xml:id="uuid_f54eef44-87e2-4219-b28b-616b7a69c2c9"/>8. APR. 1935</note>
         </p>
